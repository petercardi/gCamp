  <%= csrf_meta_tags %>
</head>
<body>
  <nav class="navbar navbar-inverse">
    <div class="container">
      <a class="navbar-brand" href="#">gCamp</a>
  </nav>

<%= yield %>
</div>
<div class="container">
<footer>
  <hr>
  <%= link_to 'About', about_path%>
  |
  <%= link_to 'Terms', terms_path%>
