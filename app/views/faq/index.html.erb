<div class='container'>
<div class="page-header">
<h1 id=top>Frequently Asked Questions</h1></div>

<ul><% @questions.each do |q, a| %>
  <li><%= link_to "#{q}", "##{q}" %></li>
<% end %></ul>

<% @questions.each do |q, a| %>
  <p><h3 id="<%= q %>"><%= q %></h3></p><br>
  <p><%= a %></p>
  <%= link_to "Top", "#top" %>
<% end %>
</div>
